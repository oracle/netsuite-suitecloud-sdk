{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://raw.githubusercontent.com/oracle/netsuite-suitecloud-sdk/refs/heads/feature/PDPDEVTOOL-6007-schemas_for_suitecloud_tools/packages/schemas/sdf-installation-deployment-1.0.0.json",
	"title": "SDF Installation Deployment",
	"description": "SDF Installation Deployment Schema Definition",
	"type": "object",
	"properties": {
		"SdfInstallationDeployment": {
			"type": "object",
			"properties": {
				"isDeployed": {
					"type": "boolean",
					"default": true
				},
				"status": {
					"type": "string",
					"enum": ["TESTING", "RELEASED", "NOTSCHEDULED", "SCHEDULED", "INPROGRESS", "INQUEUE", "COMPLETED"]
				},
				"logLevel": {
					"type": "string",
					"enum": ["AUDIT", "DEBUG", "EMERGENCY", "ERROR", "INTERNAL", "SYSTEM"]
				},
				"permissionElevation": {
					"type": "string"
				},
				"script": {
					"type": "string",
					"minLength": 1
				},
				"title": {
					"type": "string",
					"minLength": 1
				},
				"scriptId": {
					"type": "string",
					"maxLength": 40,
					"pattern": "^customdeploy.+$",
					"default": "customdeploy"
				}
			},
			"required": [
				"status",
				"script",
				"title",
				"scriptId"
			],
			"additionalProperties": false
		}
	},
	"required": [
		"SdfInstallationDeployment"
	],
	"additionalProperties": false
}