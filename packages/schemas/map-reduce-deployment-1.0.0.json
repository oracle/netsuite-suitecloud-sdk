				},
				"permissionElevation": {
					"type": "string"
				},
				"frequency": {
					"type": "string",
					"enum": ["NONE", "DAY", "WEEK", "MONTH", "YEAR"]
				},
				"period": {
					"type": "integer",
					"minimum": 1,
					"maximum": 1000
				},
				"startDate": {
					"type": "string"
				},
				"startTime": {
					"type": "string"
				},
				"endDate": {
					"type": "string"
				},
				"repeat": {
