				"wednesday": {
					"type": "boolean",
					"default": false
				},
				"thursday": {
					"type": "boolean",
					"default": false
				},
				"friday": {
					"type": "boolean",
					"default": false
				},
				"saturday": {
					"type": "boolean",
					"default": false
				},
				"sunday": {
					"type": "boolean",
					"default": false
				},
				"dayOfWeek": {
					"type": "string",
					"enum": ["SUNDAY", "MONDAY", "TUESDAY", "WEDNESDAY", "THURSDAY", "FRIDAY", "SATURDAY"]
